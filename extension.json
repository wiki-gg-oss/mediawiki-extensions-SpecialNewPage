{
	"name": "Special:NewPage",
	"version": "1.2.3",

	"author": [
		"wiki.gg development team",
		"alex4401 <alex@wiki.gg>"
	],
	"url": "https://github.com/wiki-gg-oss/mediawiki-extensions-SpecialNewPage",
	"descriptionmsg": "extnewpage-desc",
	"license-name": "GPL-2.0-or-later",
	"type": "specialpage",
	"requires": {
		"MediaWiki": ">= 1.43.0",
		"platform": {
			"php": ">= 8.1"
		}
	},
	"MessagesDirs": {
		"NewPage": [
			"i18n"
		]
	},
	"ExtensionMessagesFiles": {
		"NewPageAlias": "languages/Alias.php"
	},
	"ResourceFileModulePaths": {
		"localBasePath": "modules",
		"remoteExtPath": "SpecialNewPage/modules"
	},
	"ResourceModules": {
		"ext.newpage": {
			"class": "MediaWiki\\Extension\\NewPage\\ResourceLoader\\NewPageModule",
			"es6": true,
			"dependencies": [
				"mediawiki.widgets"
			],
			"packageFiles": [
				"ext.newpage.js"
			],
			"messages": [
				"extnewpage-newpagetext-fallback",
				"title-invalid"
			]
		},
		"ext.newpage.styles": {
			"styles": [
				"ext.newpage.styles.less"
			]
		}
	},
	"AutoloadNamespaces": {
		"MediaWiki\\Extension\\NewPage\\": "src/"
	},
	"SpecialPages": {
		"NewPage": {
			"class": "MediaWiki\\Extension\\NewPage\\SpecialPages\\SpecialNewPage"
		}
	},
	"Hooks": {
		"SidebarBeforeOutput": "NewPage"
	},
    "HookHandlers": {
        "NewPage": {
            "class": "MediaWiki\\Extension\\NewPage\\Hooks"
        }
    },
	"config_prefix": "NewPage",
	"manifest_version": 2
}
